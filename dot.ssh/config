Host 192.168.*.*
 StrictHostKeyChecking no
 UserKnownHostsFile=/dev/null
 ControlMaster no
 ControlPath ~/.ssh/control-%r@%h:%p
 ControlPersist 0

Host 172.21.*.*
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

Host rpi0w-dev
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host digitalrooster
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  User root
  IdentityFile ~/.ssh/id_digitalrooster
